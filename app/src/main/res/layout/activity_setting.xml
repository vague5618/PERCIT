                    android:adjustViewBounds="true"
                    android:background="@drawable/setting_sub3_unfocus" />


                <TextView
                    android:id="@+id/setting_sub3_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@id/setting_sub3"
                    android:layout_alignLeft="@id/setting_sub3"
                    android:layout_alignRight="@id/setting_sub3"
                    android:layout_alignTop="@id/setting_sub3"
                    android:gravity="center"
                    android:textColor="#2e313f"
                    android:textSize="16.67px"
                    android:textStyle="bold" />


                <ImageView
                    android:id="@+id/setting_sub4"
                    android:layout_width="119dp"
                    android:layout_height="107dp"
                    android:layout_marginLeft="254dp"
                    android:layout_marginTop="122dp"
                    android:adjustViewBounds="true"
                    android:background="@drawable/setting_sub4_unfocus" />


                <TextView
                    android:id="@+id/setting_sub4_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@id/setting_sub4"
                    android:layout_alignLeft="@id/setting_sub4"
                    android:layout_alignRight="@id/setting_sub4"
                    android:layout_alignTop="@id/setting_sub4"
                    android:gravity="center"
                    android:textColor="#2e313f"
                    android:textSize="16.67px"
                    android:textStyle="bold" />


                <ImageView
                    android:id="@+id/setting_sub5"
                    android:layout_width="107dp"
                    android:layout_height="119dp"
                    android:layout_marginLeft="320dp"
                    android:layout_marginTop="175dp"
                    android:adjustViewBounds="true"
                    android:background="@drawable/setting_sub5_unfocus" />


                <TextView
                    android:id="@+id/setting_sub5_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@id/setting_sub5"
                    android:layout_alignLeft="@id/setting_sub5"
                    android:layout_alignRight="@id/setting_sub5"
