                    android:textColor="#2e313f"
                    android:textSize="16.67px"
                    android:textStyle="bold" />


                <ImageView
                    android:id="@+id/setting_kick1"
                    android:layout_width="117dp"
                    android:layout_height="84dp"
                    android:layout_marginLeft="83dp"
                    android:layout_marginTop="479dp"
                    android:adjustViewBounds="true"
                    android:background="@drawable/setting_kick1_unfocus" />

                <TextView
                    android:id="@+id/setting_kick1_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@id/setting_kick1"
                    android:layout_alignLeft="@id/setting_kick1"
