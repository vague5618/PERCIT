<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/activity_handler"
            android:layout_width="30dp"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:clickable="true" />

    </RelativeLayout>


    <LinearLayout
        android:layout_width="1280dp"
        android:layout_height="727dp"
        android:background="@drawable/setting_back_total"
        android:orientation="horizontal">


        <FrameLayout
            android:layout_width="509dp"
            android:layout_height="727dp"
            android:background="@drawable/setting_back">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/setting_sub1"
                    android:layout_width="107dp"
                    android:layout_height="118dp"
                    android:layout_marginLeft="78dp"
                    android:layout_marginTop="300dp"
                    android:adjustViewBounds="true"
                    android:background="@drawable/setting_sub1_unfocus" />

                <TextView
                    android:id="@+id/setting_sub1_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@id/setting_sub1"
                    android:layout_alignLeft="@id/setting_sub1"
                    android:layout_alignRight="@id/setting_sub1"
                    android:layout_alignTop="@id/setting_sub1"
                    android:gravity="center"
                    android:textColor="#2e313f"
                    android:textSize="16.67px"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/setting_sub2"
                    android:layout_width="107dp"
                    android:layout_height="119dp"
                    android:layout_marginLeft="78dp"
                    android:layout_marginTop="175dp"
                    android:adjustViewBounds="true"
                    android:background="@drawable/setting_sub2_unfocus" />

                <TextView
                    android:id="@+id/setting_sub2_text"
                    android:layout_width="wrap_content"
